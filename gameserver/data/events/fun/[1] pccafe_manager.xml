<!DOCTYPE list SYSTEM "events.dtd">
<list>
	<!--формат даты HH:mm dd.MM.yyyy-->
	<event id="1" name="PCCafe Manager" impl="Fun">
		<parameter name="start-pattern" value="18:00 08.04.2012" />
		<parameter name="stop-pattern" value="18:15 21.04.2012" />
		<on_start>
			<spawn name="spawn_group" />
